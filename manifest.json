{
    "manifest_version": 3,
    "name": "JD Keyword Extractor",
    "description": "Extract important resume keywords from job descriptions and build a tailored resume.",
    "version": "1.1",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "16.png"
    },
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    }
}